{
    "name": "engwag",
    "displayName": "engwag",
    "description": "Train Simulator Syntax Highlighter",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "engwag",
            "aliases": ["EngWag", "engwag"],
            "extensions": [".eng",".wag"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "engwag",
            "scopeName": "source.engwag",
            "path": "./syntaxes/engwag.tmLanguage.json"
        }]
    }
}